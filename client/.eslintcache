rea\n                name=\"commentText\"\n                placeholder=\"Add your comment...\"\n                value={commentText}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Comment\n              </button>\n            </div>\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to share your thoughts. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default CommentForm;\n","/Users/brandonmccauley/workspace/project3/chatAndCatch/client/src/pages/chatbot.js",["85"],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":18,"column":8,"nodeType":"92","messageId":"93","endLine":18,"endColumn":15},{"ruleId":"94","severity":1,"message":"95","line":41,"column":1,"nodeType":"96","endLine":41,"endColumn":34},{"ruleId":"90","severity":1,"message":"97","line":41,"column":15,"nodeType":"92","messageId":"93","endLine":41,"endColumn":19},{"ruleId":"90","severity":1,"message":"97","line":19,"column":15,"nodeType":"92","messageId":"93","endLine":19,"endColumn":19},{"ruleId":"90","severity":1,"message":"98","line":1,"column":8,"nodeType":"92","messageId":"93","endLine":1,"endColumn":13},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"no-unused-vars","'chatBot' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration","'data' is assigned a value but never used.","'react' is defined but never used.","no-global-assign","no-unsafe-negation